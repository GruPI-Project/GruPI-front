{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "ES5", "ScriptHost", "ES2015"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowJs": true,
    "jsx": "preserve",
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["vite/client"]
  },
  "include": ["env.d.ts", "src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"]
}
